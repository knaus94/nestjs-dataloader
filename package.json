{
  "name": "@dessly/nestjs-dataloader",
  "version": "2.0.2",
  "description": "Quick and easy GraphQL DataLoaders for NestJS 11+",
  "keywords": [
    "nest",
    "nestjs",
    "graphql",
    "dataloader",
    "envelop",
    "yoga"
  ],
  "license": "MIT",
  "homepage": "https://github.com/knaus94/nestjs-dataloader#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/knaus94/nestjs-dataloader.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint"
  },
  "dependencies": {
    "dataloader": "^2.2.3",
    "rxjs": "^7.8.1",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0",
    "@nestjs/graphql": "^13.0.0",
    "@envelop/core": "^5.0.0",
    "graphql": "^16.0.0",
    "reflect-metadata": "^0.2.0"
  },
  "devDependencies": {
    "@envelop/types": "^5.2.1",
    "@nestjs/cli": "^11.0.7",
    "@nestjs/common": "^11.1.2",
    "@nestjs/core": "^11.1.2",
    "@nestjs/graphql": "^13.1.0",
    "@types/node": "^22.15.21",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.5.3",
    "reflect-metadata": "^0.1.14",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.2",
    "typescript": "~5.3.3"
  }
}
