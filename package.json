{
  "name": "@dessly/nestjs-dataloader",
  "version": "2.0.11",
  "description": "Quick and easy GraphQL Yoga DataLoaders for NestJS 11+",
  "keywords": [
    "nest",
    "nestjs",
    "graphql",
    "dataloader",
    "envelop",
    "yoga"
  ],
  "license": "MIT",
  "homepage": "https://github.com/knaus94/nestjs-dataloader#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/knaus94/nestjs-dataloader.git"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "lint": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint"
  },
  "dependencies": {
    "dataloader": "^2.2.3",
    "rxjs": "^7.8.2",
    "tslib": "^2.8.1"
  },
  "peerDependencies": {
    "@envelop/core": ">=5.3.2",
    "@nestjs/common": ">=11.1.6",
    "@nestjs/core": ">=11.1.6",
    "@nestjs/graphql": ">=13.1.0",
    "graphql": ">=16.11.0",
    "reflect-metadata": ">=0.2.2"
  },
  "devDependencies": {
    "@envelop/types": "^5.2.1",
    "@nestjs/cli": "^11.0.10",
    "@nestjs/common": "^11.1.6",
    "@nestjs/core": "^11.1.6",
    "@nestjs/graphql": "^13.1.0",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.0",
    "@typescript-eslint/parser": "^8.44.0",
    "typescript-eslint": "^8.44.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "typescript": "~5.9.2"
  }
}
